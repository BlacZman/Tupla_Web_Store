@page
@model Tupla_Web_Store.Pages.Analysis.BestSellerModel
@{
    ViewData["Title"] = "BestSeller";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}

<h1>5 Best seller game of all time</h1>

<table class="table">
    <thead>
        <tr>
            <td>
                Name
            </td>
            <td>
                Units
            </td>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model.test.Take(5))
        {
            <tr>
                <td>
                    @item.Key
                </td>
                <td>
                    @item.Value
                </td>
            </tr>
        }
    </tbody>
</table>